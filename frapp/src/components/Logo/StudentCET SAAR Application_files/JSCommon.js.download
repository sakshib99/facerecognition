                    $(strDistObjID).val(mdistrict_id);
                    //$(strDistObjID).change();
                    FillTalukasByID(strDistObjID, strTalukaObjID, mdistrict_id, mtaluka_id);

                }
            }
            document.getElementById('load').style.visibility = "hidden";
        }
    });
}

